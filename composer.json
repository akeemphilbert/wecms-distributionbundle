{
    "name": "wepala/wecms-distributionbundle",
    "description": "Base bundle for WeCMS Distributions",
    "keywords": ["distribution","configuration"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Akeem Philbert",
            "email": "akeem.philbert@wepala.com"
        }
    ],

    "require": {
        "php": ">=5.3.3",
        "wepala/wecms-adminbundle": "@dev",
        "wepala/wecms-userbundle": "@dev",
        "sensio/distribution-bundle": "~3.0"
    },
    "autoload": {
        "psr-0": { "WeCMS\\DistributionBundle": "" }
    },
    "target-dir": "WeCMS/DistributionBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev"
}